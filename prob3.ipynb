    "import numpy as np\n",
    "def comb(n,r):\n",
    "    if (r==0 or r==n):\n",
    "        return 1\n",
    "    else:\n",
    "        return (comb(n-1,r-1)+comb(n-1,r))\n",
    "\n",
    "n = int(input(\"Enter n: \"))\n",
    "r = int(input(\"Enter r: \"))\n",
    "c = comb(n,r)\n",
    "print(\"Combination is: \",c)"
   ]
