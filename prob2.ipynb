    "import numpy as np\n",
    "\n",
    "def sqrt_recursive(x, n):\n",
    "    if n == 0:\n",
    "        return x\n",
    "    else:\n",
    "        return np.sqrt(6 + sqrt_recursive(x, n-1))\n",
    "\n",
    "result = sqrt_recursive(6, 1000)\n",
    "print(result)\n"
   ]
